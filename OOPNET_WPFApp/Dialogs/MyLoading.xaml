<Window x:Class="OOPNET_WPFApp.Dialogs.MyLoading"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OOPNET_WPFApp.Dialogs"
        xmlns:props="clr-namespace:OOPNET_WPFApp.Properties"
        mc:Ignorable="d"
        Title="{x:Static props:Resources.Loading_Loading}" Height="250" Width="350"
		WindowStartupLocation="CenterScreen">
    <Grid>
		<Label Content="{x:Static props:Resources.Loading_Loading}" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="32" />
	</Grid>
</Window>
